<Window x:Class="EnsembleCommander.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EnsembleCommander"
        mc:Ignorable="d"
        Title="MainWindow" Height="520" Width="940" Loaded="Window_Loaded" Unloaded="Window_Unloaded">
    <StackPanel Orientation="Horizontal">
        <StackPanel Width="300">
            <Button Name="OnMIDI" Content="Midi再生" Height="80" Click="MidiOn_Click"/>
            <Button Name="OffMIDI" Content="Midi停止" Height="80" Click="MidiOff_Click"/>
            <Button Height="160"/>
            <Button Height="160"/>
        </StackPanel>
        <Grid Width="640">
            <Image Name="imageColor" Height="480" Width="640"/>
            <Canvas x:Name="CanvasFaceParts" Height="480" Width="640"/>
        </Grid>
    </StackPanel>
</Window>
